@model IEnumerable<qlks.Models.ThuePhong>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaKH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayDen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayDi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ThanhToan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaKH)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NgayDen)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NgayDi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ThanhToan)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaKH }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaKH }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaKH })
    </td>
</tr>
}

</table>
